<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="200dp"
    android:background="@drawable/doc_background"
    android:paddingStart="10dp"
    android:paddingLeft="8dp"
    android:paddingTop="8dp"
    android:paddingEnd="8dp"
    android:paddingRight="8dp"
    tools:ignore="MissingDefaultResource"
    tools:layout_editor_absoluteX="-3dp"
    tools:layout_editor_absoluteY="29dp">


        <TextView
            android:id="@+id/Doc_Name"
            android:layout_width="129dp"
            android:layout_height="28dp"
            android:text="TextView"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="@color/white"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintBaseline_toBaselineOf="@+id/doc_rep1"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/Doc_Time"
            android:layout_width="128dp"
            android:layout_height="31dp"
            android:text="TextView"
            android:textColor="@color/white"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintBaseline_toBaselineOf="@+id/doc_rep3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/doc_rep3" />

        <TextView
            android:id="@+id/doc_rep1a"
            android:layout_width="116dp"
            android:layout_height="31dp"
            android:layout_marginStart="104dp"
            android:layout_marginTop="36dp"
            android:text="@string/Doc_Visitor"
            android:textColor="@color/white"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/Owner_Name"
            android:layout_width="126dp"
            android:layout_height="30dp"
            android:layout_marginEnd="4dp"
            android:text="TextView"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="@color/white"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintBaseline_toBaselineOf="@+id/doc_rep1a"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/doc_rep1"
            android:layout_width="109dp"
            android:layout_height="25dp"
            android:layout_marginStart="104dp"
            android:layout_marginTop="28dp"
            android:text="Doctor Name :-"
            android:textColor="@color/white"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/doc_rep1a" />

        <TextView
            android:id="@+id/Doc_rep2"
            android:layout_width="121dp"
            android:layout_height="29dp"
            android:layout_marginStart="104dp"
            android:layout_marginTop="4dp"
            android:text="Date :-"
            android:textColor="@color/white"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/doc_rep1" />

        <TextView
            android:id="@+id/Doc_Date"
            android:layout_width="129dp"
            android:layout_height="30dp"
            android:text="TextView"
            android:textColor="@color/white"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintBaseline_toBaselineOf="@+id/Doc_rep2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/Doc_rep2" />

        <TextView
            android:id="@+id/doc_rep3"
            android:layout_width="125dp"
            android:layout_height="28dp"
            android:layout_marginStart="104dp"
            android:text="Time :-"
            android:textColor="@color/white"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Doc_rep2"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/doc_rep4"
            android:layout_width="120dp"
            android:layout_height="28dp"
            android:layout_marginStart="104dp"
            android:layout_marginBottom="20dp"
            android:text="Dog Count :-"
            android:textColor="@color/white"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/doc_rep3"
            app:layout_constraintVertical_bias="0.333" />

        <TextView
            android:id="@+id/Doc_Dog_Count"
            android:layout_width="126dp"
            android:layout_height="28dp"
            android:text="TextView"
            android:textColor="@color/white"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintBaseline_toBaselineOf="@+id/doc_rep4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/doc_rep4" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="138dp"
            android:layout_height="184dp"
            android:layout_marginStart="4dp"
            android:layout_marginBottom="4dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/doc_rep1"
            app:layout_constraintHorizontal_bias="0.538"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/doctorpopupimg" />

</androidx.constraintlayout.widget.ConstraintLayout>